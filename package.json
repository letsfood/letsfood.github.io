{
	"name": "letsfood.github.io",
	"version": "0.0.0",
	"description": "It's how people find a place to eat and drink. Try it now! Shake your phone!",
	"homepage": "https://letsfood.space",
	"repository": "letsfood/letsfood.github.io",
	"license": "MIT",
	"author": "CJ Patoilo <cjpatoilo@gmail.com>",
	"private": "true",
	"engines": {
		"node": ">=4"
	},
	"dependencies": {
		"milligram": "latest",
		"opbeat-js": "latest"
	},
	"devDependencies": {
		"backstopjs": "^3.0.36",
		"bowerless": "^0.0.6",
		"browser-sync": "^2.27.10",
		"cname": "^0.0.2",
		"copycopy": "0.0.2",
		"csscompile": "^0.2.0",
		"editorconfig-tools": "^0.1.1",
		"eslint": "^4.4.1",
		"eslint-config-styled": "0.0.2",
		"gh-pages": "^1.0.0",
		"htmlcompile": "^0.3.0",
		"husky": "^0.14.3",
		"imgcompile": "^0.0.0",
		"jscompile": "^0.0.2",
		"npm-run-all": "^4.0.2",
		"onchange": "^3.0.2",
		"opn-cli": "^3.1.0",
		"plato": "^1.7.0",
		"pwa-prepare": "0.0.5",
		"rimraf": "^2.5.4",
		"sass-lint": "^1.10.2",
		"surge": "^0.23.1"
	},
	"scripts": {
		"start": "run-s build watch",
		"build": "run-s build:clean && run-p build:cname build:fonts build:templates build:styles build:scripts build:images build:dependencies",
		"build:clean": "rimraf dist",
		"build:cname": "cname dist",
		"build:fonts": "copycopy src/fonts dist",
		"build:templates": "htmlcompile src dist",
		"build:styles": "csscompile src/styles dist",
		"build:scripts": "jscompile src/scripts dist",
		"build:images": "imgcompile src/images dist/images",
		"build:dependencies": "bowerless dist vendor",
		"watch": "run-p watch:templates watch:styles watch:scripts watch:images watch:dependencies serve",
		"watch:fonts": "onchange 'src/fonts' -- run-p build:fonts",
		"watch:templates": "onchange 'src/**/*.{pug,jade,ejs,haml,hbs,hogan,html}' -- run-p build:templates",
		"watch:styles": "onchange 'src/styles' -- run-p build:styles",
		"watch:scripts": "onchange 'src/scripts' -- run-p build:scripts",
		"watch:images": "onchange 'src/images' -- run-p build:images",
		"watch:dependencies": "onchange 'package.json' -- run-p build:dependencies",
		"serve": "browser-sync --no-notify start -s dist -f dist",
		"lint": "run-p lint:scripts lint:styles lint:editorconfig",
		"lint:styles": "sass-lint -c .sasslintrc src/styles/*.sass --verbose --no-exit",
		"lint:scripts": "eslint . --ignore-path .gitignore",
		"lint:editorconfig": "editorconfig-tools check .",
		"test": "run-p build",
		"complexity": "run-p complexity:styles complexity:scripts",
		"complexity:styles": "opn http://www.cssstats.com/stats?url=http://localhost:3000",
		"complexity:scripts": "plato -r -d test/complexity -e .eslintrc -x .json src/**/*.js && opn test/complexity/index.html",
		"compare": "backstop test --configPath=backstop.conf.js",
		"reference": "backstop reference --configPath=backstop.conf.js",
		"publish": "pwa-prepare dist && gh-pages -d dist -b master",
		"prepush": "run-s build publish",
		"precommit": "run-p lint"
	}
}
